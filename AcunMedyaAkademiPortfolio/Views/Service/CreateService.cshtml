﻿@model AcunMedyaAkademiPortfolio.Models.TblProject
@{
    ViewBag.Title = "CreateService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Proje Ekleme Sayfası</h2>

<form method="post">
    <label>Proje Başlığı</label>
    <input type="text" class="table table-bordered" name="Title" />
    <br />
    <label>Proje Açıklaması</label>
    <textarea rows="3" class="table table-bordered" name="Description"></textarea>
    <br />
    <label>Resim Yolu</label>
    <input type="text" class="table table-bordered" name="ImageUrl" />
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>